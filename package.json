{
  "name": "asgardex",
  "productName": "ASGARDEX",
  "version": "1.30.5",
  "description": "WALLET AND EXCHANGE CLIENT FOR THORCHAIN, MAYACHAIN, CHAINFLIP",
  "main": "index.js",
  "scripts": {
    "start:web": "craco start",
    "dev": "concurrently --kill-others --success first \"yarn:dev:*\"",
    "force-rebuild-native-deps": "rimraf ./node_modules/prebuild-install",
    "dev:app": "cross-env NODE_OPTIONS='--max_old_space_size=8192 --trace-deprecation' BROWSER=none yarn start:web",
    "dev:electron": "cross-env NODE_OPTIONS=--max_old_space_size=8192 wait-on tcp:127.0.0.1:3000 && nodemon -I --watch ./src/main/ --exec electron .",
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=8192 CI=false PUBLIC_URL=./ craco build",
    "prebuild": "./node_modules/.bin/webpack --config webpack.main.js --mode production",
    "test": "./node_modules/.bin/jest",
    "package": "yarn prebuild && yarn build && yarn package:electron",
    "package:electron": "yarn force-rebuild-native-deps && electron-builder build -c.extraMetadata.main=build/electron.js --config .electronbuildrc",
    "package:ci": "yarn postinstall && yarn prebuild && yarn build && yarn package:electron --publish always",
    "postinstall": "git config blame.ignoreRevsFile .git-blame-ignore-revs && yarn force-rebuild-native-deps && electron-builder install-app-deps",
    "lint": "trunk check --filter=eslint --filter=prettier",
    "check:trunk": "cross-env-shell ./scripts/trunk-check.sh",
    "generate:erc20whitelist": "./node_modules/.bin/ts-node --project tsconfig.scripts.json ./scripts/generateERC20Whitelist.ts",
    "generate:avaxerc20whitelist": "./node_modules/.bin/ts-node --project tsconfig.scripts.json ./scripts/generateAVAXtokenlist.ts",
    "generate:bscerc20whitelist": "./node_modules/.bin/ts-node --project tsconfig.scripts.json ./scripts/generateBSCtokenlist.ts",
    "generate:arberc20whitelist": "./node_modules/.bin/ts-node --project tsconfig.scripts.json ./scripts/generateARBtokenlist.ts",
    "generate:baseErc20whitelist": "./node_modules/.bin/ts-node --project tsconfig.scripts.json ./scripts/generateBASEtokenlist.ts",
    "storybook": "NODE_OPTIONS=\"--max-old-space-size=4096\" storybook dev -p 9009 ",
    "build-storybook": "NODE_OPTIONS=\"--max-old-space-size=8192\" storybook build",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
